int main() {
  int m[2][3] = { {1, 4, 2}, {3, 6, 8} };
  m[0][0] = 9;
  printf("%d", m[0][0]);  // Now outputs 9 instead of 1
 
  return 0;
}





#include <stdio.h>

int main() {
  int matrix[2][3] = { {1, 4, 2}, {3, 6, 8,4,4} };

  int i, j;
  for (i = 0; i <3 ; i++) {
    for (j = 0; j < 3; j++) {
      printf("%d\n", matrix[i][j]);
    }
  }
  
  return 0;
}
